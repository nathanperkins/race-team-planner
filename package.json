  "license": "UNLICENSED",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "prepare": "prisma generate",
    "generate-secret": "node -e \"console.log(require('crypto').randomBytes(32).toString('base64'))\"",
    "setup-env": "npx tsx scripts/setup-env.ts",
    "postinstall": "husky && npm run setup-env",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.564.0",
    "next": "^16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "p-retry": "^7.1.1",
    "prisma-field-encryption": "^1.6.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-heading-id": "^1.0.1",
    "remark-html": "^16.0.1",
    "remark-rehype": "^11.1.2",
    "zod": "^4.3.6"
