# .env example
#
# See README.md for instructions on populating these values.


# Application Branding
APP_TITLE=""
# Override the server locale (default: en-US).
APP_LOCALE=""
# Override the server timezone (default: America/Los_Angeles). Used for server-rendered dates, like Discord thread titles.
APP_TIMEZONE=""
# URL (optional) for reporting feedback and bugs. Adds a link in the sidebar.
# For Discord forum post, use discord://-/channels/<guild_id>/<channel_or_thread_id>.
FEEDBACK_URL=""

# Logging Configuration
# Override the log level (default: debug in development, info in production).
# Valid values: debug, info, warn, error
LOG_LEVEL=""

# Database connection that could use connection pooling.
DATABASE_URL="postgresql://postgres:password@localhost:5432/postgres"

# Direct connection to the database. Used for migrations.
DIRECT_URL="postgresql://postgres:password@localhost:5432/postgres"

# Next Auth (v5) (automatically generated by setup-env)
AUTH_SECRET=""

# Prisma Field Encryption (automatically generated by setup-env)
PRISMA_FIELD_ENCRYPTION_KEY=""

# Cron Secret (automatically generated by setup-env)
CRON_SECRET=""

# Backup Encryption Key (automatically generated by setup-env)
BACKUP_ENCRYPTION_KEY=""

# Discord OAuth
AUTH_DISCORD_ID=""
AUTH_DISCORD_SECRET=""

# Discord Membership Check (Required for access restriction)
# To get the Bot Token, go to the Discord Developer Portal -> Bot -> Reset Token
DISCORD_GUILD_ID=""
DISCORD_BOT_TOKEN=""
DISCORD_ADMIN_ROLE_IDS="" # Comma-separated list of Discord Role IDs for Admin access

# Discord Notifications (Optional) is for race registration and event notifications.
# To get a channel ID, enable Developer Mode in Discord, right-click a channel -> Copy ID
DISCORD_NOTIFICATIONS_CHANNEL_ID=""
# Discord Events Forum ID (Optional) is used to posting event and team discussion threads.
DISCORD_EVENTS_FORUM_ID=""



# iRacing Data API
IRACING_CLIENT_ID=""
IRACING_CLIENT_SECRET=""
IRACING_USERNAME=""
IRACING_PASSWORD=""
