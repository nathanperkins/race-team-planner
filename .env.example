# .env example
#
# See README.md for instructions on populating these values.

# Database connection that could use connection pooling.
DATABASE_URL="postgresql://postgres:password@localhost:5432/postgres"

# Direct connection to the database. Used for migrations.
DIRECT_URL="postgresql://postgres:password@localhost:5432/postgres"

# Next Auth (v5) (automatically generated by setup-env)
AUTH_SECRET=""

# Prisma Field Encryption (automatically generated by setup-env)
PRISMA_FIELD_ENCRYPTION_KEY=""

# Cron Secret (automatically generated by setup-env)
CRON_SECRET=""

# Discord OAuth
AUTH_DISCORD_ID=""
AUTH_DISCORD_SECRET=""

# Discord Membership Check (Required for access restriction)
# To get the Bot Token, go to the Discord Developer Portal -> Bot -> Reset Token
DISCORD_GUILD_ID=""
DISCORD_BOT_TOKEN=""
DISCORD_ADMIN_ROLE_IDS="" # Comma-separated list of Discord Role IDs for Admin access

# iRacing Data API
IRACING_CLIENT_ID=""
IRACING_CLIENT_SECRET=""
IRACING_USERNAME=""
IRACING_PASSWORD=""
